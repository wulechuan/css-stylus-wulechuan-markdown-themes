{
  "name": "@wulechuan/css-stylus-markdown-themes",
  "author": "wulechuan <wulechuan@live.com>",
  "license": "WTFPL",
  "version": "6.4.11",
  "description": "A collection of CSS themes for HTML contents generated via MarkDowns.",
  "directories": {
    "source": "source",
    "dist": "dist",
    "test": "tests",
    "doc": "documents"
  },
  "main": "index.js",
  "scripts": {
    "eslint": "eslint    ./.eslintrc.js    ./gulpfile.esm.js/**/*.js    ./documents/examples/*.js    ./tests/*.js    ./source/module/*.js    ./source/themes/js/**/*.js    ./global-config.js    ./index.js",
    "firefox": "start    firefox    ./tests/output/example-htmls-with-snapshot-helpers/example.en-us.default-light-colored-theme.html    ./tests/output/example-htmls-with-snapshot-helpers/example.en-us.default-dark-colored-theme.html    ./tests/output/example-htmls-with-snapshot-helpers/example.zh-hans-cn.default-light-colored-theme.html    ./tests/output/example-htmls-with-snapshot-helpers/example.zh-hans-cn.default-dark-colored-theme.html",
    "open-dev-htmls": "start        ./tests/output/theme-in-developement/default-theming-example.--DEV--.en-us.html    &&    start       ./tests/output/theme-in-developement/default-theming-example.--DEV--.zh-hans-cn.html",
    "clean": "gulp                  --silent  cleanAll",
    "clean2": "gulp                           cleanAll",
    "_build": "gulp                 --silent  buildAllThemesOnce",
    "_build2": "gulp                          buildAllThemesOnce",
    "_update-example-htmls": "gulp  --silent  updateExampleHTMLs",
    "_update-example-htmls2": "gulp           updateExampleHTMLs",
    "start": "gulp                  --silent  devSingleThemeOnce    &&    npm  run  open-dev-htmls    &&    gulp  --silent  devSingleTheme",
    "start2": "gulp                           devSingleThemeOnce    &&    npm  run  open-dev-htmls    &&    gulp            devSingleTheme",
    "_test-package-interfaces": "node  ./tests/test-interfaces.js",
    "_test-all-without-linting-without-browser": "npm  run  _test-package-interfaces                &&    npm  run  _build2    &&    npm  run  _update-example-htmls2    &&    gulp  devSingleThemeOnce",
    "update-example-htmls": "npm                  run  _update-example-htmls                        &&    npm  run  firefox",
    "update-example-htmls2": "npm                 run  _update-example-htmls2                       &&    npm  run  firefox",
    "test": "npm    run  eslint        &&    npm  run  _test-all-without-linting-without-browser    &&    npm  run  firefox    &&    npm  run  open-dev-htmls",
    "build": "npm   run  eslint        &&    npm  run  _build                                       &&    npm  run  update-example-htmls",
    "build2": "npm  run  eslint        &&    npm  run  _build2                                      &&    npm  run  update-example-htmls2",
    "before-git-commit": "npm    test",
    "prepublishOnly": "npm     test"
  },
  "keywords": [
    "markdown",
    "html",
    "article",
    "typography",
    "theme",
    "theming",
    "styling",
    "style",
    "skin",
    "shading",
    "coloring",
    "stylus",
    "css",
    "可读性改善",
    "层叠样式表",
    "样式",
    "文章样式",
    "文章外观",
    "文章美化",
    "文字排版",
    "文字排版美学",
    "文章“皮肤”",
    "文章中的代码着色",
    "用于美化文章的样式"
  ],
  "dependencies": {
    "globby": "^11.0.0"
  },
  "devDependencies": {
    "@wulechuan/generate-html-via-markdown": "^2.9.5",
    "@wulechuan/gulp-classical-task-cycle": "^0.3.0",
    "chalk": "^3.0.0",
    "cssnano": "^4.1.10",
    "del": "^5.1.0",
    "esm": "^3.2.25",
    "fs-extra": "^8.1.0",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-pipe": "^1.0.4",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-stylus": "^2.7.0",
    "gulp-terser": "^1.2.0"
  },
  "homepage": "https://github.com/wulechuan/wulechuan-css-stylus-themes-for-htmls-via-markdowns#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wulechuan/wulechuan-css-stylus-themes-for-htmls-via-markdowns.git"
  },
  "bugs": {
    "url": "https://github.com/wulechuan/wulechuan-css-stylus-themes-for-htmls-via-markdowns/issues"
  }
}
