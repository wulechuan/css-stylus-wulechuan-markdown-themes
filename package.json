{
  "name": "@wulechuan/css-stylus-markdown-themes",
  "author": "wulechuan <wulechuan@live.com>",
  "license": "WTFPL",
  "version": "6.4.5",
  "description": "A collection of CSS themes for HTML contents generated via MarkDowns.",
  "directories": {
    "source": "source",
    "dist": "dist",
    "test": "tests",
    "doc": "documents"
  },
  "main": "index.js",
  "scripts": {
    "eslint": "eslint    ./.eslintrc.js    ./gulpfile.esm.js/**/*.js    ./documents/examples/*.js    ./tests/*.js    ./source/module/*.js    ./source/themes/js/**/*.js    ./global-config.js    ./index.js",
    "firefox": "start    firefox    ./tests/output/example-htmls-with-snapshot-helpers/example.en-us.default-light-colored-theme.html    ./tests/output/example-htmls-with-snapshot-helpers/example.en-us.default-dark-colored-theme.html    ./tests/output/example-htmls-with-snapshot-helpers/example.zh-hans-cn.default-light-colored-theme.html    ./tests/output/example-htmls-with-snapshot-helpers/example.zh-hans-cn.default-dark-colored-theme.html",
    "openDevHTMLs": "start          ./tests/output/theme-in-developement/default-theming-example.--DEV--.en-us.html    &&    start       ./tests/output/theme-in-developement/default-theming-example.--DEV--.zh-hans-cn.html",
    "clean": "gulp                --silent  cleanAll",
    "clean2": "gulp                         cleanAll",
    "_build": "gulp               --silent  buildAllThemesOnce",
    "_build2": "gulp                        buildAllThemesOnce",
    "_updateExampleHTMLs": "gulp  --silent  updateExampleHTMLs",
    "_updateExampleHTMLs2": "gulp           updateExampleHTMLs",
    "start": "gulp                --silent  devSingleThemeOnce    &&    npm  run  openDevHTMLs    &&    gulp  --silent  devSingleTheme",
    "start2": "gulp                         devSingleThemeOnce    &&    npm  run  openDevHTMLs    &&    gulp            devSingleTheme",
    "_testPackageInterfaces": "node  ./tests/test-interfaces.js",
    "_testAllWithoutLintingWithoutBrowser": "npm  run  _testPackageInterfaces                  &&    npm  run  _build2    &&    npm  run  _updateExampleHTMLs2    &&    gulp  devSingleThemeOnce",
    "updateExampleHTMLs": "npm                    run  _updateExampleHTMLs                     &&    npm  run  firefox",
    "updateExampleHTMLs2": "npm                   run  _updateExampleHTMLs2                    &&    npm  run  firefox",
    "test": "npm    run  eslint        &&    npm  run  _testAllWithoutLintingWithoutBrowser    &&    npm  run  firefox    &&    npm  run  openDevHTMLs",
    "build": "npm   run  eslint        &&    npm  run  _build                                  &&    npm  run  updateExampleHTMLs",
    "build2": "npm  run  eslint        &&    npm  run  _build2                                 &&    npm  run  updateExampleHTMLs2",
    "beforeGitCommit": "npm    test",
    "prepublishOnly": "npm     test"
  },
  "keywords": [
    "markdown",
    "html",
    "article",
    "typography",
    "theme",
    "theming",
    "styling",
    "style",
    "skin",
    "shading",
    "coloring",
    "stylus",
    "css"
  ],
  "dependencies": {
    "@wulechuan/generate-html-via-markdown": "v2.9.1",
    "globby": "^10.0.2"
  },
  "devDependencies": {
    "@wulechuan/gulp-classical-task-cycle": "^0.3.0",
    "chalk": "^2.4.2",
    "cssnano": "^4.1.10",
    "del": "^3.0.0",
    "esm": "^3.2.25",
    "fs-extra": "^8.1.0",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-pipe": "^1.0.4",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-stylus": "^2.7.0",
    "gulp-terser": "^1.2.0"
  },
  "homepage": "https://github.com/wulechuan/wulechuan-css-stylus-themes-for-htmls-via-markdowns#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wulechuan/wulechuan-css-stylus-themes-for-htmls-via-markdowns.git"
  },
  "bugs": {
    "url": "https://github.com/wulechuan/wulechuan-css-stylus-themes-for-htmls-via-markdowns/issues"
  }
}
