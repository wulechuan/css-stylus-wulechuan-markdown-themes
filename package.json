{
  "name": "@wulechuan/css-stylus-markdown-themes",
  "author": "wulechuan <wulechuan@live.com>",
  "license": "WTFPL",
  "version": "5.9.6",
  "description": "A collection of CSS themes for Markdown generated HTML contents.",
  "directories": {
    "source": "source",
    "dist": "dist",
    "test": "tests",
    "doc": "docs"
  },
  "main": "index.js",
  "scripts": {
    "prepublishOnly": "npm run build && npm test",
    "eslint": "eslint    ./.eslintrc.js    ./gulpfile.esm.js/**/*.js    ./docs/examples/*.js    ./tests/*.js    ./source/module/*.js    ./source/themes/js/**/*.js    ./global-config.js    ./index.js",
    "start": "gulp  --silent devSingleTheme",
    "start2": "gulp          devSingleTheme",
    "_build": "gulp --silent buildAllThemesOnce",
    "_build2": "gulp         buildAllThemesOnce",
    "clean": "gulp  --silent cleanAll",
    "clean2": "gulp          cleanAll",
    "updateExampleHTMLs": "gulp    --silent    updateExampleHTMLs",
    "updateExampleHTMLs2": "gulp               updateExampleHTMLs",
    "build": "npm  run eslint    &&    npm run _build    &&    npm run updateExampleHTMLs",
    "build2": "npm run eslint    &&    npm run _build2",
    "test": "npm   run eslint    &&    node  ./tests/test-interfaces.js"
  },
  "keywords": [
    "markdown",
    "html",
    "article",
    "typography",
    "theme",
    "theming",
    "styling",
    "style",
    "skin",
    "shading",
    "coloring",
    "stylus",
    "css"
  ],
  "dependencies": {
    "globby": "^10.0.1"
  },
  "devDependencies": {
    "@wulechuan/generate-html-via-markdown": "^1.8.4",
    "@wulechuan/gulp-classical-task-cycle": "^0.2.2",
    "chalk": "^2.4.2",
    "cssnano": "^4.1.10",
    "del": "^3.0.0",
    "esm": "^3.2.25",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-pipe": "^1.0.4",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-stylus": "^2.7.0",
    "gulp-terser": "^1.2.0"
  },
  "homepage": "https://github.com/wulechuan/wulechuan-css-stylus-themes-for-htmls-via-markdowns#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wulechuan/wulechuan-css-stylus-themes-for-htmls-via-markdowns.git"
  },
  "bugs": {
    "url": "https://github.com/wulechuan/wulechuan-css-stylus-themes-for-htmls-via-markdowns/issues"
  }
}
