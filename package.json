{
  "name": "@wulechuan/css-stylus-markdown-themes",
  "author": "wulechuan <wulechuan@live.com>",
  "license": "WTFPL",
  "version": "5.4.1-prepared",
  "description": "A collection of CSS themes for Markdown generated HTML contents.",
  "directories": {
    "doc": "docs"
  },
  "main": "index.js",
  "scripts": {
    "eslint": "eslint    ./.eslintrc.js    ./gulpfile.esm.js/**/*.js    ./test/*.js    ./source/module/*.js    ./source/themes/js/**/*.js    ./global-config.js    ./index.js",
    "start": "gulp --silent buildAndWatch",
    "start2": "gulp         buildAndWatch",
    "_build": "gulp --silent buildOnce",
    "build": "npm run _build    &&    set updateExampleDocs=true    &&    node  ./test/try-theme-lookings.js",
    "build2": "gulp          buildOnce",
    "clean": "gulp --silent clean",
    "clean2": "gulp         clean",
    "dev-1": "set theOnlyStylToBuild=vscode/vscode-_default && gulp buildOnce",
    "test": "npm  run  eslint    &&    node  ./test/test-index-interfaces.js    &&    node  ./test/try-theme-lookings.js",
    "test1": "set testSingle=true && node  ./test/try-theme-lookings.js"
  },
  "keywords": [
    "markdown",
    "html",
    "article",
    "typography",
    "theme",
    "theming",
    "styling",
    "style",
    "skin",
    "shading",
    "coloring",
    "stylus",
    "css"
  ],
  "homepage": "https://github.com/wulechuan/css-stylus-wulechuan-markdown-themes#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wulechuan/css-stylus-wulechuan-markdown-themes.git"
  },
  "bugs": {
    "url": "https://github.com/wulechuan/css-stylus-wulechuan-markdown-themes/issues"
  },
  "dependencies": {
    "globby": "^10.0.1"
  },
  "devDependencies": {
    "@wulechuan/generate-html-via-markdown": "^1.0.3",
    "chalk": "^2.4.2",
    "cssnano": "^4.1.10",
    "del": "^3.0.0",
    "esm": "^3.2.25",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-pipe": "^1.0.4",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-stylus": "^2.7.0",
    "gulp-terser": "^1.2.0"
  }
}
