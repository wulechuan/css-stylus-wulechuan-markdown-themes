.wlc-parenthesis,
.wlc-square-bracket,
.wlc-curly-brace {
    color: #666;
}

.wlc-function-name,
.hljs-function .hljs-title { // javascript: function "name"
    color: #fb5;
}

.wlc-escape-char,
.hljs-string .wlc-escape-char {
    color: #c75;
}

.hljs-regexp .wlc-escape-char {
    color: #4af;
}

.hljs-regexp .wlc-digit-pair-between-curly-braces {

    .digit {
        color: #f63;
    }

    .comma {
        color: #666;
    }
}
