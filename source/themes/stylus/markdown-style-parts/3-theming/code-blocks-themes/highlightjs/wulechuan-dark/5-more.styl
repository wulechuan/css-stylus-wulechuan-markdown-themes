.punctuation {
    color: #666;

    &.parenthesis, &.square-bracket, &.curly-brace {}

    &.parenthesis {
        &.open-parenthesis {}
        &.close-parenthesis {}
    }

    &.bracket {
        &.open-bracket {}
        &.close-bracket {}
    }

    &.curly-brace {
        &.open-curly-brace {}
        &.close-curly-brace {}
    }


    &.colon {}
    &.semi-colon {}
    &.comma {}
    &.period {}


    &.greater-than-sign,
    &.greater-than-or-equal-to-sign,
    &.less-than-sign,
    &.less-than-or-equal-to-sign {
        color: #fb8;
    }
    &.tripple-equal-signs {
        color: #fba;
    }
    &.exclamation-mark-and-double-equal-signs {
        color: #a35;
    }

    &.exclamation-mark-and-equal-sign,
    &.double-equal-signs {
        color: #f33;
        background-color: #f333;
    }

    &.double-pipes {
        color: #949;
    }
    &.double-ampersands {
        color: #f0f;
    }
    &.pipe {
        color: cyan;
    }
    &.exclamation-marks {
        &.even-count {
            color: #abed69;
        }
        &.odd-count {
            color: #f15;
        }
    }


    &.equal-sign {
        color: #678;
    }


    &.question-mark {}

    &.asterisk,
    &.forward-slash,
    &.percentage-sign,
    &.plus-sign,
    &.plus-plus-sign,
    &.minus-sign,
    &.minus-minus-sign,
    &.minus-sign {
        color: #f79;
    }

    &.negative-sign {
        color: #9f3;
    }
    &.positive-sign {
        color: #39f;
    }

    &.arrow-function-sign {
        color: #7ae;
    }
}


.hljs-number.color-hex-value {
    color: #37f;
}


.wlc-function-name,
.hljs-function .hljs-title { // javascript: function "name"
    color: #f9f;
}


.wlc-built-in-method-name {
    color: #9d6;
}

.wlc-escape-char, // [ DEPREGATED ]
.escape-char {
    // color: #c75;
    color: #4ac;

    .slash {
        opacity: 0.1;
    }

    .escaped-char {
    }
}



// ------ strings (begin) --------------------------------

.hljs-string {

    .wlc-string-quote, // [ DEPREGATED ]
    .string-quote {
        color: #886;

        &.open-quote {}
        &.close-quote {}
    }

    &.empty-string .wlc-string-quote, // [ DEPREGATED ]
    &.empty-string .string-quote,
    .wlc-string-body,  // [ DEPREGATED ]
    .string-body {
        // color: #face79;
        color: #ff9;
    }

    .wlc-escape-char, // [ DEPREGATED ]
    .escape-char {
        color: #c75;

        .slash {
        }

        .escaped-char {
        }
    }
}

.language-typescript,
.language-ts,
.language-javascript,
.language-js,
.javascript, // <script>
.language-xml,
.language-html {

    .hljs-string {
        color: #ff9;
    }
}

.language-typescript,
.language-ts,
.language-javascript,
.language-js,
.javascript, // <script>
.css, // <style>
.language-css {
    // no xml, no html

    .hljs-string .wlc-string-body {
        background-color: #ffff9919;
    }
}

// ------ strings (end) ----------------------------------




.hljs-regexp {

    .regexp-close-quote,
    .regexp-open-quote {
        color: #858;
    }

    &.regexp-illegal {

        .regexp-close-quote,
        .regexp-open-quote {
            color: #fff;
            background-color #f00;
        }
    }

    .wlc-escape-char, // [ DEPREGATED ]
    .escape-char,
    & {
        color: #f6e;
    }

    .regexp-options {
        color: #3e1;
    }

    .regexp-selector-input-begin,
    .regexp-selector-input-end {
        color: #f15;
    }

    .regexp-body {
        background-color: rgba(#f6e, 0.08);
        // background-color: rgba(#fff, 0.06);
    }

    .wlc-escape-char, // [ DEPREGATED ]
    .escape-char {
        // color: #f75;

        .slash {
        }

        .escaped-char {
        }
    }


    .regexp-selector-char {

        &.wlc-escape-char, // [ DEPREGATED ]
        &.escape-char,
        & {
            color: #4f9;
        }

        &.wlc-escape-char, // [ DEPREGATED ]
        &.escape-char {

            .slash {
                opacity: 0.79;
            }

            .escaped-char {
            }
        }
    }

    .regexp-control-char {

        &.wlc-escape-char, // [ DEPREGATED ]
        &.escape-char,
